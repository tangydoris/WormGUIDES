<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" type="AnchorPane" xmlns="http://javafx.com/javafx/8">
   <padding>
      <Insets bottom="10.0"/>
   </padding>
   <VBox fx:id="mainVBox" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
      </padding>
      <HBox>
         <Region maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
         <Text fx:id="nameText" fontSmoothingType="LCD" strokeType="OUTSIDE" strokeWidth="0.0" text="Cell Name"
               textAlignment="CENTER" wrappingWidth="150.0">
            <font>
               <Font size="14.0"/>
            </font>
         </Text>
         <Region layoutX="38.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
                 maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
      </HBox>
      <HBox>
         <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
         <Button fx:id="info" mnemonicParsing="false" onAction="#showInfoAction" text="More Info">
            <font>
               <Font size="14.0"/>
            </font>
         </Button>
         <Region layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
      </HBox>
      <HBox>
         <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
         <Button fx:id="color" mnemonicParsing="false" text="Color This Cell">
            <font>
               <Font size="14.0"/>
            </font>
         </Button>
         <Region layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
      </HBox>
      <HBox>
         <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
         <Button fx:id="colorNeighbors" mnemonicParsing="false" text="Color Neighbors">
            <font>
               <Font size="14.0"/>
            </font>
         </Button>
         <Region layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
      </HBox>
      <HBox fx:id="expressesHBox" layoutX="15.0" layoutY="118.0">
         <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
         <Button fx:id="expresses" mnemonicParsing="false" text="Gene Expression">
            <font>
               <Font size="14.0"/>
            </font>
         </Button>
         <Region layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
      </HBox>
      <HBox fx:id="wiredToHBox" layoutX="15.0" layoutY="156.0">
         <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
         <Button fx:id="wiredTo" mnemonicParsing="false" text="Wired To">
            <font>
               <Font size="14.0"/>
            </font>
         </Button>
         <Region layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
      </HBox>
   </VBox>
</fx:root>
